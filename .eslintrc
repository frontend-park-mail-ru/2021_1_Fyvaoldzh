{
    "plugins": ["prettier", "import", "@typescript-eslint"],
    "env": {
        "browser": true
    },
    "extends": ["airbnb-typescript/base"],
    "rules": {
        "max-len": [2, {"code": 120}],
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-restricted-globals": 0,
        "class-methods-use-this": 0,
        "no-return-assign": 0,
        "no-case-declarations": 0,
        "no-plusplus": 0
    },
    "parserOptions": {
      "project": "./tsconfig.json"
    }
}
